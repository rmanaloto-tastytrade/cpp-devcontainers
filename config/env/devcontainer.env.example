# Devcontainer script defaults (edit and copy to devcontainer.env)
DEVCONTAINER_REMOTE_HOST=myhost.example.com
DEVCONTAINER_REMOTE_USER=myuser
DEVCONTAINER_SSH_PORT=9222
DEVCONTAINER_SSH_KEY=$HOME/.ssh/id_ed25519
# Optional: choose branch name instead of numeric clang (stable|qualification|development)
# CLANG_BRANCH=development
CLANG_VARIANT=21
GCC_VERSION=15
REQUIRE_P2996=0

# Optional: defaults for deploy script fallbacks
# DEFAULT_REMOTE_HOST=myhost.example.com
# DEFAULT_REMOTE_USER=myuser
